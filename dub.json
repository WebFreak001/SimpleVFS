{
    "name": "simplevfs",
    "description": "Minimalist virtual file system for game development",
    "copyright": "Copyright Â© 2011-2014, Ferdinand Majerech",
    "authors": ["Ferdinand Majerech"],
    "importPaths": ["source"],
    "license": "Boost 1.0",
    "homepage": "https://github.com/Zardoz89/SimpleVFS",

    "buildTypes":
    {
        "debug": { "buildOptions": ["debugMode", "debugInfoC"] },
        "release": { "buildOptions": ["releaseMode", "optimize", "inline", "noBoundsCheck"] },
        "profile": { "buildOptions": ["releaseMode", "optimize", "noBoundsCheck", "debugInfoC"] }
    },
    "subPackages": [
      "examples/getting_started",
      "examples/vfsfile"
    ],
    "toolchainRequirements": {
        "dub": ">=1.19.0",
        "frontend": ">=2.090"
    }
}

